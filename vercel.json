{
    "version": 2,
    "builds": [
      {
        "src": "keychain/package.json",
        "use": "@vercel/next"
      },
      {
        "src": "backend/api/*.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/backend/api/(.*)",
        "dest": "/api/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/keychain/$1"
      }
    ]
  }
  